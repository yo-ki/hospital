<script type="text/javascript" src="<%=path %>/static/js/tj.js"></script><script type="text/javascript">
    
    $(function(){
        // 列表类别筛选
        $(".morebtn").click(function(){
            var ddHeight = $(this).next().height();
            if($(this).hasClass("more2")){
                $(this).parent().css("height","25px");
                $(this).removeClass("more2").html("更多");
            }else{
                $(this).parent().animate({height:ddHeight},"normal");
                $(this).addClass("more2").html("收起");
            };
            return false;
        });
    });

    // 关注/取关ajax操作
    $(".addMark").click(function(){
        var this_doctor_id = $(this).attr('doctor_id');
        var is_marked = ($(this).hasClass('diconA2') == false) ? 1 : 0;
        var msg = is_marked ? '关注' : '取消关注';
        var _self = this;
        $.ajax({
            url: JYUrl('doctors', 'doMark', {docid:this_doctor_id, marked:is_marked}),
            type: 'get',
            dataType: 'json',
            success: function (rel) {
                if(rel == '2'){ //未登录，请登录
                    userLoginBox('1');
                    return false;
                }else if (rel == 1) {
                    if($(_self).hasClass('diconA2')){
                        $(_self).removeClass('diconA2').attr('title',msg);
                    }else{
                        $(_self).addClass('diconA2').attr('title',msg);
                    }
                }

                _alert(msg+(rel==1 ? '成功' : '失败'));
            }
        });
    })
    
</script>

